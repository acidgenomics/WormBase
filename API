# API for worminfo package

## Exported functions

cherrypick(identifier, format = "keyword", ahringer384 = TRUE, ahringer96 = FALSE, orfeome96 = TRUE, plates = NULL)
eggnog(identifier)
ensemblBasic(gene = NULL)
ensemblGeneOntology(gene = NULL, nest = TRUE)
ensemblInterpro(gene = NULL, nest = TRUE)
ensemblPeptide(peptide = NULL)
gene(identifier, format = "gene", select = NULL)
geneExternal(identifier)
geneOntology(identifier)
geneReport(identifier, format = "gene")
microplate(plate = 1, well = 96, control = 0, prefix = NULL)
rnai(identifier, format = "clone", proteinCoding = TRUE)
uniprot(identifier)
worfdbData(worfdbHTML)
worfdbHTML(sequence)
